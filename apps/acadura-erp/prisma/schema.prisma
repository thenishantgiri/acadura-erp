generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Tenant {
  id         String   @id @default(uuid())
  name       String
  deployMode String   @map("deploy_mode")
  createdAt  DateTime @default(now()) @map("created_at")
}
