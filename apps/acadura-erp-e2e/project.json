{
  "name": "acadura-erp-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "implicitDependencies": ["acadura-erp"],
  "targets": {
    "e2e": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
      "options": {
        "jestConfig": "apps/acadura-erp-e2e/jest.config.ts",
        "passWithNoTests": true
      },
      "dependsOn": ["acadura-erp:build", "acadura-erp:serve"]
    }
  }
}
